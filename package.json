{
	"name": "rtlcheatsheet",
	"displayName": "RTLCheatSheet",
	"description": "Cheat Sheet for the React Testing Library",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"license": "MIT",
	"icon": "images/octopus-128x128.png",
	"activationEvents": [
		"onCommand:rtlcheatsheet.rtlCheatsheet",
		"onLanguage:javascript",
		"onLanguage:typescript"
	],
	"publisher": "will-nemo",
	"repository": {
		"type": "git",
		"url": "https://github.com/will-nemo/rtlcheatsheet"
	},
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "rtlcheatsheet.rtlCheatsheet",
				"title": "Open React Testing Library Cheatsheet"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.47.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
